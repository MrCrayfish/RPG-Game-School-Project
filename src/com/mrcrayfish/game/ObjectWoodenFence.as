package  {		import flash.display.Bitmap;	import flash.display.BitmapData;	import flash.geom.Rectangle;	import flash.geom.Point;		public class ObjectWoodenFence extends Overlay{		public function ObjectWoodenFence() {			super(GameSprite.fence_1.getSprite(),true);		}				override public function renderObject(level:Level, x:int, y:int, layer:BitmapData, topLayer:BitmapData)		{			var left:Boolean = level.getObject(x - 1, y) == this;			var right:Boolean = level.getObject(x + 1, y) == this;			var up:Boolean = level.getObject(x, y - 1) == this;			var down:Boolean = level.getObject(x, y + 1) == this;						if(level.getTile(x - 1, y) != null && !left) left = level.getTile(x - 1, y).isSolid();			if(level.getTile(x + 1, y) != null && !right) right = level.getTile(x + 1, y).isSolid();			if(level.getTile(x, y - 1) != null && !up) up = level.getTile(x, y - 1).isSolid();			if(level.getTile(x, y + 1) != null && !down) down = level.getTile(x, y + 1).isSolid();									var sprite:Bitmap;			if (! left && ! right && up && ! down)			{				sprite = GameSprite.fence_2.getSprite();			}			else if (!left && !right && !up && down)			{				sprite = GameSprite.fence_3.getSprite();			}			else if (left && !right && !up && !down)			{				sprite = GameSprite.fence_5.getSprite();			}			else if (!left && right && !up && !down)			{				sprite = GameSprite.fence_4.getSprite();			}			else if (!left && !right && up && down)			{				sprite = GameSprite.fence_6.getSprite();			}			else if (left && right && !up && !down)			{				sprite = GameSprite.fence_7.getSprite();			}			else if (!left && right && !up && down)			{				sprite = GameSprite.fence_8.getSprite();			}			else if (left && !right && !up && down)			{				sprite = GameSprite.fence_9.getSprite();			}			else if (!left && right && up && !down)			{				sprite = GameSprite.fence_10.getSprite();			}			else if (left && !right && up && !down)			{				sprite = GameSprite.fence_11.getSprite();			}			else if (!left && right && up && down)			{				sprite = GameSprite.fence_12.getSprite();			}			else if (left && right && !up && down)			{				sprite = GameSprite.fence_13.getSprite();			}			else if (left && right && up && !down)			{				sprite = GameSprite.fence_14.getSprite();			}			else if (left && !right && up && down)			{				sprite = GameSprite.fence_15.getSprite();			}			else if (left && right && up && down)			{				sprite = GameSprite.fence_16.getSprite();			}			else			{				sprite = GameSprite.fence_1.getSprite();			}						//trace("trying to render");			layer.copyPixels(sprite.bitmapData, new Rectangle(0, 0, sprite.width, sprite.height), new Point( x * 16 + xOffset, y * 16 + yOffset), null, null, true);		}	}	}