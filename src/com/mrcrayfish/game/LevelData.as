package {	import flash.events.Event;	import flash.net.URLRequest;	import flash.net.URLLoader;	public class LevelData	{		public static var commonHouse:LevelData = new LevelData("common_house");		public static var homeVillage:LevelData = new LevelData("village1");		public var data:Array = new Array();		public var width:int;		public var height:int;		public function LevelData(mapName:String)		{			loadData(mapName);		}				public function loadData(mapName:String)		{			trace("Loading data for " + mapName);			var file:URLRequest = new URLRequest(mapName + ".txt");;			var loader:URLLoader = new URLLoader();			loader.addEventListener(Event.COMPLETE, process);			loader.load(file);			trace("Finished loading data for " + mapName);		}				private function process(e:Event)		{			trace("Process Data");			trace(e.target.data);			var rows:Array = e.target.data.split("\n");			for (var y = 0; y < rows.length; y++)			{				this.data[y] = new Array();				var rowData:Array = rows[y].split(",");				for (var x = 0; x < rowData.length; x++)				{					trace("ID for tile at X:"+x+ " Y:"+y+" is "+rowData[x]);					this.data[y][x] = rowData[x];				}			}			this.width = data[0].length;			this.height = data.length;		}	}}