package {	import flash.display.Bitmap;	import flash.display.BitmapData;	import flash.geom.Rectangle;	import flash.geom.Point;		import PropWoodenDoor;	import PropWoodenWall;	public class GameObject	{		public static var wooden_door:PropWoodenDoor = new PropWoodenDoor();		public static var wooden_wall:GameObject = new PropWoodenWall();				private var solid:Boolean;		private var tileSprite:Bitmap;		public function GameObject(){}				public function setSprite(sprite:Bitmap):void		{			this.tileSprite = sprite;		}				public function setSolid(flag:Boolean):void		{			this.solid = flag;		}				public function onUse(player:Player)		{					}		public function isSolid():Boolean		{			return solid;		}		public function renderObject(x:int, y:int, layer:BitmapData):void		{			layer.copyPixels(tileSprite.bitmapData, new Rectangle(0, 0, 16, 16), new Point( x * 16, y * 16 ));		}	}}