package  {		public class ObjectSign extends Overlay{				private var message:String;		public function ObjectSign(message:String) {			super(GameSprite.sign.getSprite(),false);			this.message = message;		}				public function getMessage():String {			return message;		}				override public function onUse(player:Player)		{			trace(message);		}		override public function isSelectable():Boolean		{			return true;		}	}	}